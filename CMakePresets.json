{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "windows-msvc-rel",
      "displayName": "Windows MSVC Release",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "SLIC3R_STATIC": "ON",
        "ENABLE_SLA": "ON",
        "USE_OPENVDB": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "rel",
      "configurePreset": "windows-msvc-rel"
    }
  ]
}